
    public static void main(String [] args) {
        compute(1, 5);
        compute(2, 5);
        compute(3, 5);
    }
    
    public static void compute(int x, int y) {
        if (x == 1){
            System.out.println("The square root of " + y + " is " + y * y);
        } else if (x == 2) {
          System.out.println("The cube of " + y + " is " + y * y);      
        } else {
            System.out.println(y);
        }
        
    }
}
